<h1>Editing Teacher #<%= @teacher.id %></h1>

<form action="/teachers/<%= @teacher.id %>/update">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" value="<%= @teacher.name  %>" type="text" />
  </div>
  <div>
    <label for="email">email</label><br />
    <input id="email" name="email" value="<%= @teacher.email  %>" type="text" />
  </div>
  <div>
    <label for="img">img</label><br />
    <input id="img" name="img" value="<%= @teacher.img  %>" type="text" />
  </div>
  <div>
    <label for="bio">bio</label><br />
    <input id="bio" name="bio" value="<%= @teacher.bio  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>


